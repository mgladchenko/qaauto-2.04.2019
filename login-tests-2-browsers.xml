<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Login Test Suite">
    <test name="Firefox" >
        <parameter name="browserName" value="Firefox"/>
        <classes>
            <class name="test.LoginTest"></class>
        </classes>
    </test>

    <test name="Chrome" >
        <parameter name="browserName" value="Chrome"/>
        <classes>
            <class name="test.LoginTest"></class>
        </classes>
    </test>

</suite>
